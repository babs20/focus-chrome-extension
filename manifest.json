{
  "name": "Focus",
  "description": "Focus on getting actual work done.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./power-16x16.png",
    "32": "./power-32x32.png",
    "48": "./power-48x48.png",
    "128": "./power-128x128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "https://www.google.com/*", "storage"]
}
