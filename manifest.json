{
  "name": "Focus",
  "description": "Focus on getting actual work done.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/power-16x16.png",
    "32": "./icons/power-32x32.png",
    "48": "./icons/power-48x48.png",
    "128": "./icons/power-128x128.png"
  },
  "background": {
    "scripts": ["./background.js"],
    "persistent": true
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"]
}
